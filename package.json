{
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "next": "10.0.8",
    "next-redux-wrapper": "^6.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "styled-components": "^5.2.1",
    "svg-sprite-loader": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.8",
    "@types/svg-sprite-loader": "^3.9.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.11",
    "node-sass": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-scss": "^3.19.0",
    "svgo": "^2.2.2",
    "svgo-loader": "^3.0.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:types",
      "pre-push": "npm run test",
      "post-merge": "sh hooks/post-merge.sh",
      "post-rewrite": "sh hooks/post-merge.sh"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "main": "index.js",
  "name": "fin-manager",
  "private": true,
  "scripts": {
    "build": "rm -rf .next && next build",
    "dev": "next dev",
    "export": "npm run build && next export",
    "lint:ts": "npm run prettier && eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:scss": "stylelint --fix 'src/**/*.scss'",
    "lint:types": "tsc -p . --noEmit",
    "prettier": "prettier  --write **/*.{ts,tsx,js}",
    "start": "NODE_ENV=production PORT=$PORT next start",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "test": "NODE_ENV=test jest -c test/jest.config.js",
    "test:dev": "NODE_ENV=test jest --watchAll -c test/jest.config.js"
  },
  "version": "0.1.0"
}
